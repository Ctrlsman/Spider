import requests
import base64

url = 'http://118.190.210.225:8800/v2/users/55/behavior?$user_id=2452121-78011b5a6228e4bb77df4422d75edc70d1767d259cac94d22de4f986cee2ca8a0cf42988774c4bda39f912cb810cd188'
username = 'admin'
pw = 'youliwang164ca6ccc25'

headers = {'Authorization': 'Basic ' + (base64.b64encode((username + ':' + pw).encode('utf-8'))).decode('utf-8')}
print(headers)
resp = requests.get(url, headers=headers)
print(resp.content)
